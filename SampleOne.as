package{	import flash.display.MovieClip;	/**	 * Driver class for Fla based project.	 */	public class SampleOne extends MovieClip	{		private var _model:GameBoardModel;				// View		private var _controller:GameBoardController;	// Controller		private var _board:GameBoard;					// Model				/**		 * Constructor for the driver class; calls the initialization method.		 */		public function SampleOne()		{			super();			init();		}				/**		 * Initializes the mvc pieces, adds the view to the main swf, and calls setup.		 */		private function init():void		{			_model = new GameBoardModel();			_controller = new GameBoardController(_model);			_board = new GameBoard(_model, _controller);			addChild(_board);						_controller.setup(8, 8, Utils.randomRange(4, 6));		}	}}